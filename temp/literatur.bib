@InBook{Sheppard2017,
  author    = {Sheppard, Dennis},
  title     = {Background Sync for Offline Apps with Service Workers},
  booktitle = {Beginning Progressive Web App Development: Creating a Native App Experience on the Web},
  year      = {2017},
